    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/AzulBack</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme2">
    </style>


    <style name="ThemeOverlay.MaterialComponents.TextInputEditText.OutlinedBox." parent="ThemeOverlay.MaterialComponents.TextInputEditText.OutlinedBox" >
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/AzulBack</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="startIconTintMode">@null</item>
    </style>



    <style name="ToolbarTheme" parent="ThemeOverlay.AppCompat.ActionBar">
        <item name="colorControlNormal">@color/ColorWhite</item>
        <item name="titleTextColor">@color/ColorWhite</item>
    </style>

    <style name="StyleTheme.AppCompat.Light" parent="Base.Theme.AppCompat.Light">
        <item name="color">@color/ColorWhite</item>
    </style>

